			},
			{
				"name": "Transform to Dedicated",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "Transform to Enriched",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
