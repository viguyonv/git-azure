			{
				"name": "Get Cluster State",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": {
						"value": "@concat('https://',\npipeline().parameters.p_DATABRICKS_HOST,\n'/api/2.0/clusters/get')",
						"type": "Expression"
					},
					"method": "POST",
					"headers": {
						"Authorization": {
							"value": "@concat('Bearer '\n,pipeline().parameters.p_DATABRICKS_TOKEN\n)",
							"type": "Expression"
						}
					},
					"body": {
						"value": "@concat('{ \"cluster_id\": \"',pipeline().parameters.p_DATABRICKS_CLUSTER_ID,'\" }')",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Start Cluster If state is not running",
				"type": "IfCondition",
				"dependsOn": [
					{
						"activity": "Get Cluster State",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@if(equals(activity('Get Cluster State').output.state, 'RUNNING'), 1, 0)",
						"type": "Expression"
					},
					"ifFalseActivities": [
						{
							"name": "Start Cluster",
							"type": "WebActivity",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"url": {
									"value": "@concat('https://',\npipeline().parameters.p_DATABRICKS_HOST,\n'/api/2.0/clusters/get')",
									"type": "Expression"
								},
								"method": "POST",
								"headers": {
									"Authorization": {
										"value": "@concat('Bearer '\n,pipeline().parameters.p_DATABRICKS_TOKEN\n)",
										"type": "Expression"
									}
								},
								"body": {
									"value": "@concat('{ \"cluster_id\": \"',pipeline().parameters.p_DATABRICKS_CLUSTER_ID,'\" }')",
									"type": "Expression"
								}
							}
						}
					]
				}
			}
		],
